<Project>

  <PropertyGroup>

    <RootNamespace>Arsenal.ImageMounter</RootNamespace>
    <OutputPath Condition="'$(MSBuildProjectExtension)' != '.shfbproj'">..\bin\$(Configuration)\</OutputPath>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <DisableWinExeOutputInference>true</DisableWinExeOutputInference>
    <RunAnalyzersDuringBuild>false</RunAnalyzersDuringBuild>
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
    <LangVersion Condition="'$(MSBuildProjectExtension)' == '.csproj'">9.0</LangVersion>
    <OptionInfer>On</OptionInfer>
    <OptionStrict>On</OptionStrict>
    <OptionExplicit>On</OptionExplicit>
    <DebugType>portable</DebugType>
    <Deterministic>false</Deterministic>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <DiscUtilsPath Condition="'$(TargetFramework)' == 'net40'">$(SolutionDir)..\..\3rdparty\DiscUtils\$(Configuration)\net40</DiscUtilsPath>
    <DiscUtilsPath Condition="'$(TargetFramework)' == 'net47'">$(SolutionDir)..\..\3rdparty\DiscUtils\$(Configuration)\net46</DiscUtilsPath>
    <DiscUtilsPath Condition="'$(TargetFramework.CompareTo(`net5`))' &gt;= 0">$(SolutionDir)..\..\3rdparty\DiscUtils\$(Configuration)\netstandard2.0</DiscUtilsPath>

  </PropertyGroup>

  <PropertyGroup>

    <Title>Arsenal Image Mounter</Title>
    <Company>Arsenal Recon</Company>
    <Product>Arsenal Image Mounter</Product>
    <Copyright>Copyright Â© Arsenal Recon, 2015 - 2021</Copyright>
    <Trademark />
    <FileVersion>3.6.188.0</FileVersion>
    <ProductVersion>3.6.188.0</ProductVersion>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>

  </PropertyGroup>

</Project>
